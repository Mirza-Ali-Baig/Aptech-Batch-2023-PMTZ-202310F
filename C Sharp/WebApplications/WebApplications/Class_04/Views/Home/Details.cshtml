@model List<Employee>;

<h1>
    Employee Details Page
</h1>

<table class="table table-bordered table-striped">
    <thead>
        <tr>
            <th>
                ID
            </th> 
            <th>
                Name
            </th> 
            <th>
                Email
            </th>
            <th>
                Salary
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach (Employee item in Model)
        {
            <tr>
                <td>@item.Id</td>
                <td>@item.Name</td>
                <td>@item.Email</td>
                <td>@item.Salary</td>
            </tr>
        }
    </tbody>
</table>